<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%
	out.println("hello");
	String oneday = (String)request.getParameter("oneday");
	Cookie popupCookie = new Cookie("oneday",oneday);
	popupCookie.setPath(request.getContextPath());
	popupCookie.setMaxAge(600);
	response.addCookie(popupCookie);
%>