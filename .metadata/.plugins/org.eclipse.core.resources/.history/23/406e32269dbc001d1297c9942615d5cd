<%
	String oneday = request.getParameter("oneday");
	Cookie popCookie = new Cookie("oneday",oneday);
	popCookie.setPath(request.getContextPath());
	popCookie.setMaxAge(60);
	response.addCookie(popCookie);
	System.out.println(oneday);
	out.println("FIRE2");		
%>